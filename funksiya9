// İstifadəçidən bir ədəd alın, cüt və ya tək olduğunu göstərin. 
#include <iostream>
using namespace std;
int eded(int x){
    int y;
    if(x%2==0){
        y=2;
    }
    else
    y=1;
 }
int main() {
    int f;
    f=eded(9);
    if(f==2){
        cout<<"murekkeb";
    }
    else
    cout<<"sade";
}